{
  "name": "no-chubby",
  "displayName": "no chubby",
  "description": "reminder for water intake",
  "publisher": "manu-xo5",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.46.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:no-chubby.startTimer"
  ],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "no-chubby.stopTimer",
        "title": "\"No-Chubby: Stop Timer"
      },
      {
        "command": "no-chubby.startTimer",
        "title": "\"No-Chubby: Start New Timer"
      }
    ]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "node ./test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.46.0",
    "@types/glob": "^7.1.3",
    "@types/mocha": "^8.0.4",
    "@types/node": "^12.11.7",
    "eslint": "^7.15.0",
    "glob": "^7.1.6",
    "mocha": "^8.1.3",
    "typescript": "^4.1.2",
    "vscode-test": "^1.4.1"
  }
}
